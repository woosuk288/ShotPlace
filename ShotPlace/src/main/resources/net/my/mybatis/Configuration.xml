<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
		
	<typeAliases>
		<typeAlias type="net.my.user.User" alias="User" />
		<typeAlias type="net.my.board.Board" alias="Board" />
		<typeAlias type="net.my.board.Article" alias="Article"/>
		<typeAlias type="net.my.board.AttachFile" alias="AttachFile" />
		<typeAlias type="net.my.board.Comment" alias="Comment"/>
	</typeAliases>

	<mappers>
		<mapper resource="net/my/mybatis/UserMapper.xml" />
		<mapper resource="net/my/mybatis/BoardMapper.xml" />
	</mappers>

</configuration>